{
  "name": "QRify",
  "description": "Transfer the currently selected text or current URL to a mobile device using a QR code.",
  "version": "0.0.2",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": "default-src 'self'; img-src 'self' https://chart.googleapis.com;",
  "icons": {
	"16": "icon16.png",
	"48": "icon48.png",
	"128": "icon128.png"
  },
  "manifest_version": 2
}
